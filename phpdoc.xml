<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>CROSS Applicant Manager</title>
    <parser>
        <target>public/docs</target>
        <markers>
            <item>TODO</item>
            <item>FIXME</item>
        </markers>
        <extensions>
            <extension>php</extension>
            <extension>phtml</extension>
        </extensions>
        <visibility></visibility>
    </parser>
    <transformer>
        <target>public/docs</target>
    </transformer>
    <logging>
        <level>warn</level>
        <paths>
            <default>log/{DATE}.log</default>
            <errors>log/{DATE}.errors.log</errors>
        </paths>
    </logging>
    <transformations>
<!--        <template name="zend" />-->
    </transformations>
    <files>
        <file>test.php</file>
        <file>bin/*</file>
        <directory>.</directory>
        <ignore>vendor/*</ignore>
        <ignore>test/*</ignore>
    </files>
</phpdoc>
